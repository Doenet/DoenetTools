  "scripts": {
    "start": "cd doenet_docker && docker-compose up",
    "docker:build": "cd doenet_docker && docker-compose build",
    "docker:lamp": "cd doenet_docker_lamp_only && docker-compose up",
    "docker:webpack": "cd doenet_docker_webpack_only && docker-compose up",
    "webpack": "webpack --mode development --config webpack.local.config.js",
    "build": "webpack --mode production --config webpack.deployment.config.js",
    "test": "cypress open",
    "test:all": "cypress run -b 'chrome'",
    "generate-docs:components": "babel-node generate-components-json.js",
    "docs": "babel-node test2.js",
    "deploy": "cd doenet_docker_deploy && docker-compose up",
    "publish_db": "cd doenet_docker && docker-compose up -d mysql && docker exec --privileged -w /var/lib/mysql doenet_docker_mysql_1 mysqldump --databases --add-drop-database --add-drop-table -u root -phelloworld doenet_local > ./volumes/db_init/db_template.sql",
    "reset_db": "docker exec -i doenet_docker_mysql_1 sh -c 'mysql -u root -phelloworld doenet_local' < ./doenet_docker/volumes/db_init/db_template.sql"
  },
  
