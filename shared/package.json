{
  "name": "@doenet-tools/shared",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "description": "Shared utilities for DoenetTools",
  "main": "./dist/index.js",
  "typings": "./dist/index.d.ts",
  "files": [
    "/dist"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "default": "./dist/index.js"
    }
  },
  "scripts": {
    "build": "wireit",
    "lint": "eslint --fix",
    "lint:check": "eslint"
  },
  "wireit": {
    "build": {
      "command": "vite build",
      "files": [
        "src/**/*.ts",
        "src/**/*.js",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/**/*.js",
        "dist/**/*.d.ts"
      ]
    }
  },
  "dependencies": {
    "ipfs-only-hash": "^4.0.0"
  }
}
