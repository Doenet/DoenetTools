<award credit='0.9' name='award9'>$intf_signerror1b </award>
<award allowedErrorInNumbers='0.0001' credit='0.81' name='award10'>$intf_signerror1b </award>
<award credit='0.9' name='award11'>$intf_signerror1c </award>
<award allowedErrorInNumbers='0.0001' credit='0.81' name='award12'>$intf_signerror1c </award>
<award credit='0.81' name='award13'>$intf_signerror2a </award>
<award allowedErrorInNumbers='0.0001' credit='0.73' name='award14'>$intf_signerror2a </award>
<award credit='0.81' name='award15'>$intf_signerror2b </award>
<award allowedErrorInNumbers='0.0001' credit='0.73' name='award16'>$intf_signerror2b </award>
<award credit='0.81' name='award17'>$intf_signerror2c </award>
<award allowedErrorInNumbers='0.0001' credit='0.73' name='award18'>$intf_signerror2c </award>
<award credit='0.73' name='award19'>$intf_signerror3 </award>
