  <mrow>x_0\amp = \frac{$b }{$a }.</mrow>
</md>
</p>

<p>
<m>x_1 =</m> 
<answer name='answer1'>
<award symbolicEquality='true' simplifyOnCompare="numbers">$ans1</award>
</answer>

</p>

<p>
<m>x_2 =</m> 
<answer name='answer2'>
<award symbolicEquality='true' simplifyOnCompare="numbers">$ans2</award>
</answer>

</p>

<p>
<m>x_3 =</m> 
<answer name='answer3'>
<award symbolicEquality='true' simplifyOnCompare="numbers">$ans3</award>
</answer>

</p>

<p>
<m>x_4 =</m> 
<answer name='answer4'>
<award symbolicEquality='true' simplifyOnCompare="numbers">$ans4</award>
</answer>

</p>



<solution>

<p>
<m>x_1 =  $ans1</m>
</p>

<p>
<m>x_2 =  $ans2</m>
</p>

<p>
<m>x_3 =  $ans3</m>
</p>

<p>
