<setup>
  <math name="initValue">1</math>

  <selectFromSequence from="2" to="5" name="a" />
  <selectFromSequence from="3" to="5" name="b" />
  <math name="c" simplify>$a+2</math>
  <function name="f" symbolic formula="x^($b)+$a*x-$c" />
</setup>

