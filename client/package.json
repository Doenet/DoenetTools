{
  "name": "@doenet-tools/client",
  "version": "1.0.1",
  "type": "module",
  "private": true,
  "scripts": {
    "build": "wireit",
    "dev": "cross-env NODE_ENV=production vite",
    "lint": "eslint --fix",
    "lint:check": "eslint",
    "test-cypress": "cypress open"
  },
  "wireit": {
    "build": {
      "command": "tsc && vite build",
      "files": [
        "src/**/*.ts",
        "src/**/*.js",
        "src/**/*.tsx",
        "src/**/*.jsx",
        "src/**/*.css",
        "vite.config.ts",
        "tsconfig.json"
      ],
      "output": [
        "dist/**/*.js",
        "dist/**/*.d.ts"
      ],
      "dependencies": [
        "../shared:build"
      ]
    }
  },
  "dependencies": {
    "@chakra-ui/icons": "^2.2.4",
    "@chakra-ui/react": "^2.10.9",
    "@doenet/assignment-viewer": "^0.1.0-alpha-12",
    "@doenet/doenetml-iframe": "^0.7.1",
    "@doenet/v06-to-v07": "^0.7.1",
    "@doenet-tools/shared": "*",
    "@fontsource/jost": "^5.2.6",
    "axios": "^1.11.0",
    "better-react-mathjax": "^2.4.0-beta-1",
    "browser-image-resizer": "^2.4.1",
    "cssesc": "^3.0.0",
    "export-to-csv": "^1.4.0",
    "js-cookie": "^3.0.5",
    "luxon": "^3.7.1",
    "math-expressions": "^2.0.0-alpha76",
    "nanoid": "^5.1.5",
    "papaparse": "^5.5.3",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "react-dropzone": "^14.3.8",
    "react-icons": "5.5.0",
    "react-qrcode-logo": "^3.0.0",
    "react-router": "^7.7.1",
    "react-select": "^5.10.2",
    "recharts": "^2.15.4",
    "styled-components": "6.1.19",
    "swiper": "^9.4.1"
  },
  "devDependencies": {
    "@babel/eslint-parser": "^7.28.0",
    "@babel/plugin-syntax-class-properties": "^7.12.13",
    "@esbuild-plugins/node-globals-polyfill": "^0.2.3",
    "@esbuild-plugins/node-modules-polyfill": "^0.2.2",
    "@rollup/plugin-alias": "^5.1.1",
    "@rollup/plugin-commonjs": "^28.0.6",
    "@rollup/plugin-node-resolve": "^16.0.1",
    "@rollup/plugin-terser": "^0.4.4",
    "@testing-library/react": "^16.3.0",
    "@types/cssesc": "^3.0.2",
    "@types/papaparse": "^5.3.16",
    "@types/react": "19.2.7",
    "@types/react-dom": "19.2.3",
    "@types/react-router": "^5.1.20",
    "@vitejs/plugin-react": "^5.1.1",
    "@web/test-runner": "^0.20.2",
    "cross-env": "^10.0.0",
    "esbuild": "^0.27.0",
    "eslint-plugin-jsx-a11y": "^6.10.2",
    "eslint-plugin-react": "^7.37.5",
    "eslint-plugin-react-hooks": "^5.2.0",
    "globals": "^16.3.0",
    "rollup": "^4.46.2",
    "rollup-plugin-polyfill-node": "^0.13.0"
  },
  "optionalDependencies": {
    "@esbuild/linux-arm64": "^0.25.8"
  }
}
